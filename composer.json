{
    "name": "gintonicweb/twbs-theme",
    "description": "Twitter Bootstrap default theme for GintonicCMS",
    "type": "cakephp-plugin",
    "homepage": "http://gintoniccms.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Philippe Lafrance",
            "homepage": "http://phillafrance.com",
            "role": "Author"
        }
    ],
    "require": {
        "cakephp/cakephp": "~3.0",
        "gintonicweb/requirejs": "~0.1",
        "friendsofcake/bootstrap-ui": "~0.3",
        "gintonicweb/websockets": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1"
    },
    "autoload": {
        "psr-4": {
            "TwbsTheme\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TwbsTheme\\Test\\": "tests"
        }
    }
}
